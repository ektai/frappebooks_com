{
  "name": "frappebooks_com",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Frappe <developers@frappe.io>",
  "license": "MIT",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.2.0",
    "watch": "^1.0.2"
  },
  "scripts": {
    "build-dev-css": "yarn run tailwind build ./frappebooks_com/styles/index.css -o ./frappebooks_com/public/css/style.dev.css",
    "build-prod-css": "yarn run postcss ./frappebooks_com/styles/index.css -o ./frappebooks_com/public/css/style.prod.css",
    "develop": "watch 'yarn run build-dev-css' ./ --filter=watch_files.js",
    "build": "yarn run build-prod-css"
  }
}
